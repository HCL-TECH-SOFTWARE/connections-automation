{% set total_users_orga = __mt_ldap_orga_nr_of_users | int + 1 %}
{% set total_users_orgb = __mt_ldap_orgb_nr_of_users | int + 1 %}
{% set total_users_ext_orga = __mt_ldap_orga_nr_of_ext_users | int + 1 %}
{% set total_users_ext_orgb = __mt_ldap_orgb_nr_of_ext_users | int + 1 %}

{% for counter in range(1, total_users_orga) %}
dn: cn={{ __mt_ldap_orga_userid }}{{ counter }},CN=Users,ou={{ __mt_org_unit_name }},{{ ldap_realm }}
uid: {{ __mt_ldap_orga_userid }}{{ counter }}
mail: {{ __mt_ldap_orga_userid }}{{ counter }}@{{ __ldap_user_mail_domain }}
givenName: {{ __mt_ldap_orga_givenname }}
SN: {{ __mt_ldap_orga_sn }}{{ counter }}
CN: {{ __mt_ldap_orga_givenname }} {{ __mt_ldap_orga_sn }}{{ counter }}
ibm-socialOrganizationId: 4000000001
ibm-socialOrganizationName: {{ __mt_ldap_orga_name }}
ibm-socialPersonId: {{ 1000000000+counter }}
objectclass: organizationalPerson
objectclass: inetOrgPerson
objectclass: person
objectclass: top
objectclass: ibm-socialPerson
ibm-entryuuid: {{ (1000000000+counter+4000000001) | to_uuid }}
userPassword: {{ __mt_ldap_orga_user_password }}

{% endfor %}

